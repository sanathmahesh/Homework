{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 350, "height": 350,
  "data": {
    "url": "https://raw.githubusercontent.com/sanathmahesh/Homework/main/greenhouse_gas_euro_edited.csv",
    "format": {"type": "csv"}
  },
  "transform": [
    {
      "filter":{
        "field": "country_or_area",
        "oneOf":["Australia"]
      }
    }
  ],
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "yearmonth", "field": "year",
      "axis": {"format": "%Y"}
    },
    "y": {
      "aggregate": "sum", "field": "value"
    },
    "color": {
      "field": "category",
      "type": "nominal",
      "title": "Explained by",
      "scale": {
        "domain": [
          "CO2 Emissions",
          "Greenhouse Gas(Indirect)",
          "Greenhouse Gas(Direct)",
          "Hydrofluorocarbons",
          "Methane",
          "Nitrogen",
          "Nitrous Oxide",
          "Perfluorocarbons",
          "Sulphur"
        ],
        "range": [
          "#1f77b4",
          "#ff7f0e",
          "#2ca02c",
          "#d62728",
          "#9467bd",
          "#8c564b",
          "#000000",
          "#f781bf",
          "#999999"
        ]
      },
      "legend": null
    },
    "tooltip":[
      {
        "field": "category"
      }
    ]
  }
}
